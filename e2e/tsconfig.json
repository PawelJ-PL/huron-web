{
    "compilerOptions": {
        "target": "es5",
        "lib": ["esnext", "dom"],
        "allowJs": false,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "noFallthroughCasesInSwitch": true,
        "module": "esnext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": false,
        "types": ["cypress"],
        "noEmit": true
    },
    "include": ["cypress"]
}
